{
  "name": "@leomotors/music-bot",
  "version": "0.21.0",
  "description": "discord.js music bot that can be run standalone or as a module for other bots",
  "main": "dist/index.js",
  "typings": "./dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Leomotors/music-bot"
  },
  "author": "Leomotors",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm lm-config && tsc",
    "format": "prettier --write \"{src,tests}/**/*.{js,ts}\"",
    "lint": "eslint src && prettier --check \"{src,tests}/**/*.{js,ts}\"",
    "test": "node --es-module-specifier-resolution=node tests/index.js",
    "bot": "pnpm build && node --es-module-specifier-resolution=node tests/bot.js",
    "prepack": "rimraf dist/* && pnpm build && pnpm lint && pnpm test"
  },
  "devDependencies": {
    "@discordjs/opus": "^0.8.0",
    "@leomotors/scripts": "^3.1.0",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/node": "^18.7.20",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "dotenv": "^16.0.2",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "libsodium-wrappers": "^0.7.10",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@discordjs/voice": "^0.11.0",
    "chalk": "^5.0.1",
    "cocoa-discord-utils": "^2.3.0",
    "discord.js": "^14.4.0",
    "play-dl": "^1.9.5",
    "tslib": "^2.4.0",
    "uuid": "^9.0.0"
  },
  "keywords": [
    "discord-bot",
    "music-bot",
    "discordjs",
    "cocoa-discord-utils"
  ]
}