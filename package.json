{
  "name": "@leomotors/music-bot",
  "version": "0.24.0",
  "description": "discord.js music bot that can be run standalone or as a module for other bots",
  "main": "dist/index.js",
  "typings": "./dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Leomotors/music-bot"
  },
  "author": "Leomotors",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm lm-config && tsc",
    "lint": "eslint src",
    "format": "eslint src --fix",
    "test": "node tests/index.js",
    "bot": "pnpm build && node tests/bot.js",
    "prepack": "rimraf dist/* && pnpm build && pnpm lint && pnpm test"
  },
  "devDependencies": {
    "@discordjs/opus": "^0.9.0",
    "@leomotors/scripts": "^3.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "^18.15.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "libsodium-wrappers": "^0.7.11",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@discordjs/voice": "^0.15.0",
    "chalk": "^5.2.0",
    "cocoa-discord-utils": "^2.3.0",
    "discord.js": "^14.8.0",
    "play-dl": "^1.9.6",
    "tslib": "^2.5.0",
    "uuid": "^9.0.0"
  },
  "keywords": [
    "discord-bot",
    "music-bot",
    "discordjs",
    "cocoa-discord-utils"
  ]
}
